{
  "extends": [
    "stylelint-config-sass-guidelines",
    "stylelint-config-concentric-order"
  ],
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-empty-line-before": [ "always", {
      "except": [
        "blockless-after-same-name-blockless",
        "first-nested"
      ],
      "ignore": ["after-comment"]
    }],
    "at-rule-no-unknown": null,
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "aspect-ratio"
        ]
      }
    ],
    "scss/no-global-function-names": null,
    "scss/load-no-partial-leading-underscore": null,
    "no-duplicate-selectors": true,
    "selector-attribute-quotes": "always",
    "property-no-vendor-prefix": true,
    "value-no-vendor-prefix": true,
    "function-url-quotes": "always",
    "font-weight-notation": "numeric",
    "font-family-name-quotes": "always-where-required",
    "comment-whitespace-inside": "always",
    "comment-empty-line-before": [ "always", {
      "except": ["first-nested"],
      "ignore": ["stylelint-commands"]
    }],
    "custom-property-empty-line-before": [ "always", {
      "except": [
        "after-custom-property",
        "first-nested"
      ],
      "ignore": [
        "after-comment",
        "inside-single-line-block"
      ]
    }],
    "declaration-empty-line-before": [ "always", {
      "except": [
        "after-declaration",
        "first-nested"
      ],
      "ignore": [
        "after-comment",
        "inside-single-line-block"
      ]
    }],
    "at-rule-no-vendor-prefix": true,
    "rule-empty-line-before": "always",
    "order/properties-alphabetical-order": null,
    "max-nesting-depth": 6,
    "selector-class-pattern": null,
    "order/properties-order": [
      "content",
      "display",
      "position",
      "top",
      "right",
      "bottom",
      "left",
      "z-index",
      "grid",
      "grid-area",
      "grid-template",
      "grid-template-areas",
      "grid-template-rows",
      "grid-template-columns",
      "grid-row",
      "grid-row-start",
      "grid-row-end",
      "grid-column",
      "grid-column-start",
      "grid-column-end",
      "grid-auto-rows",
      "grid-auto-columns",
      "grid-auto-flow",
      "grid-gap",
      "grid-row-gap",
      "grid-column-gap",
      "gap",
      "row-gap",
      "column-gap",
      "flex",
      "flex-grow",
      "flex-shrink",
      "flex-basis",
      "flex-flow",
      "flex-direction",
      "flex-wrap",
      "justify-content",
      "align-content",
      "align-items",
      "align-self",
      "order",
      "float",
      "clear",
      "box-sizing",
      "width",
      "min-width",
      "max-width",
      "height",
      "min-height",
      "max-height",
      "margin",
      "margin-top",
      "margin-right",
      "margin-bottom",
      "margin-left",
      "padding",
      "padding-top",
      "padding-right",
      "padding-bottom",
      "padding-left",
      "border",
      "border-width",
      "border-style",
      "border-color",
      "border-top",
      "border-top-width",
      "border-top-style",
      "border-top-color",
      "border-right",
      "border-right-width",
      "border-right-style",
      "border-right-color",
      "border-bottom",
      "border-bottom-width",
      "border-bottom-style",
      "border-bottom-color",
      "border-left",
      "border-left-width",
      "border-left-style",
      "border-left-color",
      "border-radius",
      "border-top-left-radius",
      "border-top-right-radius",
      "border-bottom-right-radius",
      "border-bottom-left-radius",
      "border-collapse",
      "border-spacing",
      "table-layout",
      "empty-cells",
      "caption-side",
      "font",
      "font-family",
      "font-size",
      "font-weight",
      "font-style",
      "font-variant",
      "font-size-adjust",
      "font-stretch",
      "font-feature-settings",
      "font-kerning",
      "font-language-override",
      "font-smoothing",
      "src",
      "line-height",
      "letter-spacing",
      "word-spacing",
      "white-space",
      "text-align",
      "text-align-last",
      "text-transform",
      "text-decoration",
      "text-decoration-line",
      "text-decoration-style",
      "text-decoration-color",
      "text-indent",
      "text-overflow",
      "text-overflow-ellipsis",
      "text-overflow-mode",
      "text-shadow",
      "text-rendering",
      "text-size-adjust",
      "text-justify",
      "text-orientation",
      "text-wrap",
      "text-emphasis",
      "text-emphasis-style",
      "text-emphasis-color",
      "text-emphasis-position",
      "text-emphasis-skip",
      "text-emphasis-clip",
      "text-emphasis-fill",
      "text-emphasis-mark",
      "text-emphasis-mark-line",
      "text-emphasis-mark-color",
      "text-emphasis-mark-size",
      "text-emphasis-mark-fill",
      "text-emphasis-mark-clip",
      "text-emphasis-position-last",
      "text-emphasis-color-last",
      "text-emphasis-style-last",
      "text-emphasis-mark-last",
      "text-emphasis-mark-line-last",
      "text-emphasis-mark-color-last",
      "text-emphasis-mark-size-last",
      "text-emphasis-mark-fill-last",
      "text-emphasis-mark-clip-last",
      "text-emphasis-position-round",
      "text-emphasis-position-space",
      "text-emphasis-position-trim",
      "text-emphasis-position-percentage",
      "vertical-align",
      "white-space",
      "word-spacing",
      "word-break",
      "word-wrap",
      "overflow",
      "overflow-x",
      "overflow-y",
      "clip",
      "clear",
      "color",
      "background",
      "background-color",
      "background-image",
      "background-repeat",
      "background-position",
      "background-size",
      "background-clip",
      "background-origin",
      "background-attachment",
      "background-blend-mode",
      "box-decoration-break",
      "object-fit",
      "object-position",
      "image-resolution",
      "transform",
      "transform-box",
      "transform-origin",
      "transform-style",
      "backface-visibility",
      "perspective",
      "perspective-origin",
      "outline",
      "outline-width",
      "outline-style",
      "outline-color",
      "outline-offset",
      "opacity",
      "visibility",
      "filter",
      "isolation",
      "mix-blend-mode",
      "box-shadow",
      "box-decoration-break",
      "overflow-wrap",
      "tab-size",
      "table-layout",
      "unicode-bidi",
      "direction",
      "columns",
      "column-count",
      "column-fill",
      "column-gap",
      "column-rule",
      "column-rule-width",
      "column-rule-style",
      "column-rule-color",
      "column-span",
      "column-width",
      "break-before",
      "break-inside",
      "break-after",
      "page-break-before",
      "page-break-inside",
      "page-break-after",
      "orphans",
      "widows",
      "zoom",
      "max-zoom",
      "min-zoom",
      "user-zoom",
      "fill",
      "fill-rule",
      "clip-rule",
      "stroke",
      "stroke-width",
      "stroke-linecap",
      "stroke-linejoin",
      "stroke-dasharray",
      "stroke-dashoffset",
      "stroke-opacity",
      "resize",
      "cursor",
      "pointer-events",
      "touch-action",
      "user-select",
      "appearance",
      "content",
      "counter-reset",
      "counter-increment",
      "quotes",
      "resize",
      "user-modify",
      "user-focus",
      "user-select",
      "user-input",
      "user-trigger",
      "user-zoom",
      "user-zoom",
      "interpolation-mode",
      "viewport-fill",
      "viewport-fill-opacity",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit",
      "viewport-fit"
    ],
    "order/order": [
      "custom-properties",
      "dollar-variables",
      {
        "type": "at-rule",
        "hasBlock": false
      },
      "declarations",
      {
        "type": "rule",
        "selector": "^&::\\w+$"
      },
      "rules",
      {
        "type": "rule",
        "selector": "^&:\\w+$"
      },
      {
        "type": "rule",
        "selector": "^&__\\w+$"
      },
      {
        "type": "rule",
        "selector": "^&--\\w+$"
      },
      {
        "type": "rule",
        "selector": "^&--\\w+_\\S+$"
      },
      {
        "type": "rule",
        "selector": "^&--\\w+-\\S+$"
      },
      {
        "type": "rule",
        "selector": "^&.\\w+$"
      }
    ]
  }
}
